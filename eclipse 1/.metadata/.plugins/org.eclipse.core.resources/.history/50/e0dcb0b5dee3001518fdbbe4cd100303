
public class CalculationsV4 {
	 public static void main(String[ ] args)
	    {
	        // Addition
	        System.out.println("Addition");
	        int iNum1=25;
	        int iNum2=9;
	        System.out.println( iNum1+ " " +" plus " +iNum2+" = "+(iNum1+iNum2) );
	        System.out.println( 43.21 + 3.14 + 5.0 );
	      
	        //System.out.println();
	        
	        // Subtraction
	        System.out.println("Subtraction");
	        int iNum3=16;
	        System.out.println(iNum3 +" minus "+ iNum2+" minus "+  iNum1+" = "+ (iNum3 - iNum2 - iNum1)  );
	        System.out.println( 3.14 - 5.0 );
	        
	        //System.out.println();
	        
	        // Multiplication
	        System.out.println("Multiplication");
	        System.out.println(iNum1 +" times "+iNum2+" = "+ (iNum1 * iNum2 )); 
	        System.out.println(3.14 * 5.0 * 5.0 );
	        //System.out.println();
	        
	        // Division
	        System.out.println("Division");
	        System.out.println( iNum2+" divided by "+iNum1+" = "+(iNum2 / iNum1 ));
	        System.out.println( 43.21 / 5.0 );
	        //System.out.println();
	         
	        // Modulus operator
	        System.out.println("Modulus");
	        System.out.println(iNum3+" modulus "+iNum2+" = "+ (iNum3 % iNum2 ));
	        System.out.println( 5.0 % 3.14 );
	        //System.out.println();
	        
	        // Exponential operator
	        System.out.println("Exponential");
	        System.out.println(iNum2+" to the power of 2 "+"+"+ (iNum2*iNum2));
	        System.out.println( 5.0 * 3.14 );
	        
	        //square root operator
	        System.out.println("square root");
	        System.out.println(iNum2+" square root "+"="+ (iNum2*0.5));
	        System.out.println( 6.3 * 0.5 );
	        // 2.02 Lab Equations
	        // 15 divided by 2.5 times -2 plus 10 / 5
	        // 234 minus (234 divided by 6 modulus 12) + 3
	        // (46.2 divided by 11) minus 3 plus 24 modulus (17 minus 2 times 3)
	        // 480 divided by 10 divided by 12 plus 200 *.5 minus 20 modulus 8
	        
	        // 2.03 Additional int Equations
	        
	        
	        //2.04 Additional double Equations
	        

}
}